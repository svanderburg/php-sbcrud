{
  "name": "svanderburg/php-sbcrud",
  "description": "Abstractions to construct web-based information systems",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
      {
          "name": "Sander van der Burg",
          "email": "svanderburg@gmail.com",
          "homepage": "http://sandervanderburg.nl"
      }
  ],
  "autoload": {
      "psr-4": { "SBCrud\\": "src/SBCrud" },
      "files": [
          "src/SBCrud/View/HTML/pager.php",
          "src/SBCrud/View/HTML/operationtoolbar.php"
      ]
  },
  "autoload-dev": {
      "psr-4": {
          "Examples\\Check\\": "examples/check/includes",
          "Examples\\ReadOnly\\": "examples/readonly/includes",
          "Examples\\Full\\": "examples/full/includes",
          "Examples\\Paged\\": "examples/paged/includes"
      }
  },
  "repositories": [
      {
          "type": "git",
          "url": "https://github.com/svanderburg/php-sbdata.git"
      },
      {
          "type": "git",
          "url": "https://github.com/svanderburg/php-sblayout.git"
      }
  ],
  "require": {
      "svanderburg/php-sblayout": "dev-master",
      "svanderburg/php-sbdata": "dev-master"
  }
}
